<mj-wrapper background-color="#F6F6F6" full-width="full-width" padding="40px 0 0">
  <%= render 'statistics' %>
  <%= render 'top-ideas' %>
  <%= render 'new-initiatives' unless new_initiatives.empty? %>
  <%= render 'successful-initiatives' unless successfull_initiatives.empty? %>
